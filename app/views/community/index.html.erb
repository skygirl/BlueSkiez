<h1>Community#index</h1>
<h1>My community of customers!</h1>
<p>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone</th>
    <th>City</th>
    <th>State</th>
</tr>
<% @customerlist .each do |c| %>
  <tr>
    <td><%= c.cust_fname %></td>
    <td><%= c.cust_lname %></td>
    <td><%= c.cust_phone %></td>
    <td><%= c.cust_city %></td>
</tr>
<% end %>
</table>
</p>
